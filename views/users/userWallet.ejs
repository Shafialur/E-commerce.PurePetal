    <style>
       
        .card {
            width: 300px;
            height: 200px;
            perspective: 1000px;
            margin-left: 38%;
        }
        .card-inner {
            width: 100%;
            height: 100%;
            transition: transform 0.5s;
            transform-style: preserve-3d;
            position: relative;
        }
        .card:hover .card-inner {
            transform: rotateY(180deg);
        }
        .card-face {
            position: absolute;
            width: 100%;
            height: 100%;
            backface-visibility: hidden;
            border-radius: 10px;
            box-shadow: 0 0 20px rgba(0, 0, 0, 0.1);
        }
        .card-front {
            background: linear-gradient(135deg, #65a9e0, #9055a2);
            display: flex;
            justify-content: center;
            align-items: center;
            border-radius: 10px;
            padding: 20px;
        }
        .card-back {
            background: linear-gradient(135deg, #65a9e0, #9055a2);
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            transform: rotateY(180deg);
            border-radius: 10px;
            padding: 20px;
        }
        .card-content {
            text-align: center;
        }
        h2, p {
            margin: 5px 0;
        }
        .buttons{
            display: flex;
            justify-content: center;
        }
    </style>
